@article{testbeam,
title = {The DESY II test beam facility},
journal = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
volume = {922},
pages = {265-286},
year = {2019},
issn = {0168-9002},
doi = {https://doi.org/10.1016/j.nima.2018.11.133},
url = {https://www.sciencedirect.com/science/article/pii/S0168900218317868},
author = {R. Diener and J. Dreyling-Eschweiler and H. Ehrlichmann and I.M. Gregor and U. Kötz and U. Krämer and N. Meyners and N. Potylitsina-Kube and A. Schütz and P. Schütze and M. Stanitzki},
keywords = {DESY, Test beam, Infrastructure},
abstract = {DESY Hamburg operates a test beam facility with three independent beam lines at the DESY II synchrotron. It is world-wide one of very few facilities providing test beams in the GeV range. To this end, it offers electron/positron beams with user-selectable momenta from 1-6GeV/c. The available infrastructure for the users is unique, including a high field solenoidal magnet and permanently installed high-precision pixel beam telescopes. This publication gives a detailed description of the facility, the available infrastructure, and the simulated and measured performance.}
}

